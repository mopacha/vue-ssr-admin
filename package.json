{
  "name": "vue-ssr-admin",
  "version": "1.0.0",
  "description": "based on vue2 koa2 webpack4 for ssr render",
  "scripts": {
    "dev": "node server/index.js",
    "start": "npm run build && cross-env NODE_ENV=production npm run dev",
    "fix-eslint": "eslint client --ext .js,.vue --fix",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.config.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mopacha/vue-ssr-admin.git"
  },
  "keywords": [
    "vue2",
    "vue-ssr",
    "ssr",
    "koa",
    "koa2",
    "webpack4"
  ],
  "author": "moofyu@163.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mopacha/vue-ssr-admin/issues"
  },
  "homepage": "https://github.com/mopacha/vue-ssr-admin#readme",
  "dependencies": {
    "orgchart": "^2.1.9",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.10",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "autoprefixer": "^9.6.4",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "element-ui": "^2.12.0",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "js-cookie": "^2.2.1",
    "json-loader": "^0.5.7",
    "koa": "^2.8.2",
    "koa-better-http-proxy": "^0.2.4",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nprogress": "^0.2.0",
    "opn": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path-to-regexp": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "stylus": "^0.54.7",
